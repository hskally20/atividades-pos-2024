<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE imobiliaria [
  <!ELEMENT imobiliaria (imovel+)>
  <!ELEMENT imovel (descricao, proprietario, endereco, caracteristicas, valor)>
  <!ELEMENT descricao (#PCDATA)>
  
  <!ELEMENT proprietario (nome, (email?, telefone*))>
  <!ELEMENT nome (#PCDATA)>
  <!ELEMENT email (#PCDATA)>
  <!ELEMENT telefone (#PCDATA)>
  
  <!ELEMENT endereco (rua, bairro, cidade, numero?)>
  <!ELEMENT rua (#PCDATA)>
  <!ELEMENT bairro (#PCDATA)>
  <!ELEMENT cidade (#PCDATA)>
  <!ELEMENT numero (#PCDATA)>
  
  <!ELEMENT caracteristicas (tamanho, numQuartos, numBanheiros)>
  <!ELEMENT tamanho (#PCDATA)>
  <!ELEMENT numQuartos (#PCDATA)>
  <!ELEMENT numBanheiros (#PCDATA)>
  
  <!ELEMENT valor (#PCDATA)>
]>
<imobiliaria>
  <imovel id='a1'>
    <descricao>Casa com quintal amplo</descricao>
    <proprietario>
      <nome>ari</nome>
      <email>ari@example.com</email>
      <telefone>123456789</telefone>
    </proprietario>
    <endereco>
      <rua>Rua das Flores</rua>
      <bairro>Jardim Primavera</bairro>
      <cidade>Cidade Nova</cidade>
      <numero>123</numero>
    </endereco>
    <caracteristicas>
      <tamanho>250.</tamanho>
      <numQuartos>4</numQuartos>
      <numBanheiros>3</numBanheiros>
    </caracteristicas>
    <valor>R$500.000,00</valor>
  </imovel>
</imobiliaria>
